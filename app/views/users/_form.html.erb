

<%= form_with model: @user do |form| %>
    <div><%= form.label :name %> <br>
    <%= form.text_field :name %></div><br>
    <div><%= form.label :email %> <br>
    <%= form.text_field :email %></div><br>
    <div><%= form.label :password %> <br>
    <%= form.text_field :password %></div><br>

    <%= form.submit %>

    <% end %>